      "/test/",
      "/dist/"
    ]
  },
  "keywords": [
    "api",
    "rest"
  ],
  "author": "Ditmar",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^27.0.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jest": "^27.2.5",
    "mongoose": "^6.0.9",
    "ts-jest": "^27.0.6",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13"
  }
}
